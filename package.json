{
  "author": "Aydrian Howard <aydrian@tabnine.com> (https://tabnine.com)",
  "bin": {
    "dominos": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.3.0",
    "conf": "^13.0.0",
    "dominos": "^3.3.1",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "description": "A CLI for ordering pizza from Domino's üçï",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20",
    "@types/prompts": "^2.4.9",
    "@types/sinon": "^17.0.3",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "memfs": "^4.9.3",
    "oclif": "^4.13.8",
    "shx": "^0.3.4",
    "sinon": "^18.0.0",
    "stdout-stderr": "^0.1.13",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/aydrian/dominos-cli-workshop",
  "license": "MIT",
  "main": "",
  "name": "dominos-cli-workshop",
  "oclif": {
    "bin": "dominos",
    "dirname": "dominos",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "order": {
        "description": "Order pizza from Domino's üçï"
      },
      "profile": {
        "description": "Manage your Dominos profile"
      },
      "store": {
        "description": "Manage your favorite Dominos store"
      },
      "track": {
        "description": "Track your Dominos orders"
      }
    }
  },
  "repository": "aydrian/dominos-cli-workshop",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "vitest",
    "typecheck": "tsc",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/aydrian/dominos-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
